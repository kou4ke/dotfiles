nnoremap [deol] <Nop>
nmap ,t [deol]

let g:deol#shell_history_path = '~/.zsh_history'
nnoremap <silent> [deol]f :Deol -split=floating<CR>
nnoremap <silent> [deol]v :Deol -split=vertical<CR>
nnoremap <silent> [deol]s :Deol -split=horizontal<CR>
tnoremap <silent> [deol]q <C-\><C-n>:q<CR>
tnoremap <silent> <C-w><C-w> <C-\><C-n><C-w>w
" nmap  bg (deol_bg)
